{
  "dashboard": {
    "id": null,
    "title": "React Application Metrics",
    "tags": ["react", "frontend"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Page Views",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(react_page_views_total[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "JavaScript Errors",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(react_js_errors_total[5m])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Bundle Size",
        "type": "stat",
        "targets": [
          {
            "expr": "react_bundle_size_bytes",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Load Time",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(react_page_load_duration_seconds_bucket[5m]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Page Load Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(react_page_load_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(react_page_load_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(react_page_load_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "P99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "API Call Duration",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(react_api_call_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "P95 API Duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "Error Rate by Page",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(react_js_errors_total[5m])",
            "refId": "A",
            "legendFormat": "{{page}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "User Interactions",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(react_user_interactions_total[5m])",
            "refId": "A",
            "legendFormat": "{{interaction_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
